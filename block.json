{
  "$schema": "https://json.schemastore.org/block.json",
  "apiVersion": 2,
  "name": "yelp-block/profile",
  "version": "3.0.0",
  "title": "Yelp Block",
  "category": "widgets",
  "icon": "star-filled",
  "description": "Easily display Yelp business reviews and ratings with a simple and intuitive WordPress block.",
  "supports": {
	"html": false
  },
  "attributes": {
	"apiKeyValid": {
	  "type": "boolean",
	  "default": false
	},
	"businessId": {
	  "type": "string",
	  "default": ""
	},
	"preview": {
	  "type": "boolean",
	  "default": false
	}
  },
  "example": {
	"attributes": {
	  "preview": true
	}
  },
  "textdomain": "yelp-block",
  "editorScript": "file:./build/yelp-block.js",
  "editorStyle": "file:./build/yelp-block.css",
  "style": "file:./build/style-yelp-block.css"
}
